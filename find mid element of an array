class Playground {
    public static void main(String[ ] args) {
        int arr[]={1,2,3,4,5,6,7,8,9,3,8};
        int mid=arr.length/2;
        int id=arr[mid];
        System.out.println(id);

    }
}
// in String 
class Playground {
    public static void main(String[ ] args) {
        String arr[]={"a","b","c","d","e","f","g"};
        int  mid=arr.length/2;
        String id=arr[mid];
        System.out.println(id);

    }
}
