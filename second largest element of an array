class Playground {
    public static void main(String[ ] args) {
        int a[]={1,2,8,4,6,9,7};
        int large = a[0];
        int seclarge = 0;
        for (int i=1;i<a.length;i++){
            if(a[i]>large){
                large=a[i];
            }
        }
        for(int i=0;i<a.length;i++){
            if(a[i]>seclarge && a[i]!=large){
                seclarge=a[i];
            }
            
        }System.out.println(seclarge);
    }
}
